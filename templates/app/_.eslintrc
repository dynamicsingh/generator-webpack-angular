{
  "extends": "eslint:recommended",
  "plugins": ["angular"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "angular/no-service-method": 0
  },
  "env": {
    "browser": true
  },
  "globals": {
    "angular": true,
    "module": true,
    "inject": true,
    "require": true
  }
}
