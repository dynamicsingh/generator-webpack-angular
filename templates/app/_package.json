{
  "name": "<%= _.slugify(appName) %>",
  "version": "0.0.1",
  "private": true,
<% if (cssPreprocessor === 'sass') { -%>
  "sasslintConfig": "src/.sass-lint.yml",
<% } -%>
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile --env dev",
    "build:prod": "rimraf dist && webpack --bail --progress --profile --env prod",
    "start": "npm-run-all --parallel serve<% if (jsonServer) { -%> json-server<% } -%>",
    "serve": "webpack-dev-server --history-api-fallback --inline --progress --hot --env dev",
    "serve:prod": "webpack-dev-server --history-api-fallback --inline --progress --hot --env prod",
    "test": "npm run-script lint",
    "lint": "eslint src/ && htmlhint src/<% if (cssPreprocessor === 'css') { -%> && csslint src/styles/<% } else if (cssPreprocessor === 'sass') { -%> && sass-lint -v -q<% } -%>"<% if (jsonServer) { -%>,
    "json-server": "json-server json-server/db.json --routes json-server/routes.json"<% } -%>
  },
  "dependencies": {
    "angular": "^1.5.8",
<% if (ngAnimate || uiFramework === 'ngMaterial' || uiFramework === 'bootstrap') { -%>
    "angular-animate": "^1.5.8",
<% } -%>
    "angular-aria": "^1.5.8",
<% if (ngCookies) { -%>
    "angular-cookies": "^1.5.8",
<% } -%>
<% if (uiFramework === 'ngMaterial') { -%>
    "angular-material": "^1.1.1",
<% } -%>
<% if (ngMessages) { -%>
    "angular-messages": "^1.5.8",
<% } -%>
<% if (ngSanitize || angularTranslate) { -%>
    "angular-sanitize": "^1.5.8",
<% } -%>
<% if ((ngTouch || uiFramework === 'bootstrap') && uiFramework !== 'ngMaterial') { -%>
    "angular-touch": "^1.5.8",
<% } -%>
<% if (resource === 'ngResource') { -%>
    "angular-resource": "^1.5.8",
<% } -%>
<% if (angularTranslate) { -%>
    "angular-translate": "^2.13.0",
    "angular-translate-loader-static-files": "^2.13.0",
<% } -%>
    "angular-ui-router": "^1.0.0-beta.3"<% if (resource === 'restangular') { %>,
<% if (uiFramework === 'ngMaterial') { -%>
    "material-design-icons": "^3.0.1",
<% } -%>
<% if (uiFramework === 'bootstrap') { -%>
    "angular-ui-bootstrap": "^2.2.0",
    "bootstrap": "^3.3.7",
<% } -%>
    "restangular": "^1.5.2"<% } %>
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "npm-run-all": "^3.1.2",
<% if (cssPreprocessor === 'css') { -%>
    "csslint": "^1.0.4",
<% } -%>
    "eslint": "^3.9.1",
    "eslint-plugin-angular": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "htmlhint": "^0.9.13",
    "json-loader": "^0.5.4",
<% if (cssPreprocessor === 'less') { -%>
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
<% } -%>
    "ng-annotate-loader": "^0.2.0",
<% if (cssPreprocessor === 'sass') { -%>
    "node-sass": "^3.10.1",
<% } -%>
    "postcss-loader": "^1.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
<% if (cssPreprocessor === 'sass') { -%>
    "sass-loader": "^4.0.2",
    "sass-lint": "^1.10.2",
<% } -%>
<% if (jsonServer) { -%>
    "json-server": "^0.9.2",
<% } -%>
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1"
  }
}
