{
  "name": "<%= _.slugify(appname) %>",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "start": "webpack-dev-server --history-api-fallback --inline --progress --hot --env dev",
    "start:prod": "webpack-dev-server --history-api-fallback --inline --progress --hot --env prod"
  },
  "dependencies": {
    "angular": "^1.5.8",
<% if (ngAnimate) { -%>
    "angular-animate": "^1.5.8",
<% } -%>
    "angular-aria": "^1.5.8",
<% if (ngCookies) { -%>
    "angular-cookies": "^1.5.8",
<% } -%>
<% if (ngMessages) { -%>
    "angular-messages": "^1.5.8",
<% } -%>
<% if (ngSanitize) { -%>
    "angular-sanitize": "^1.5.8",
<% } -%>
<% if (ngTouch) { -%>
    "angular-touch": "^1.5.8",
<% } -%>
<% if (resource === 'ngResource') { -%>
    "angular-resource": "^1.5.8",
<% } -%>
<% if (angularTranslate) { -%>
    "angular-translate": "^2.13.0",
    "angular-translate-loader-static-files": "^2.13.0",
<% } -%>
    "angular-ui-router": "^1.0.0-beta.3"<% if (resource === 'restangular') { %>,
    "restangular": "^1.5.2"<% } %>
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.7.1",
    "json-loader": "^0.5.4",
<% if (cssPreprocessor === 'less') { -%>
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
<% } -%>
    "ng-annotate-loader": "^0.2.0",
<% if (cssPreprocessor === 'sass') { -%>
    "node-sass": "^3.10.1",
<% } -%>
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
<% if (cssPreprocessor === 'sass') { -%>
    "sass-loader": "^4.0.2",
<% } -%>
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1"
  }
}
